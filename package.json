  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "prepare": "husky",
    "prepare-husky": "npx husky init; echo \"# npm test\nnpx lint-staged\" > .husky/pre-commit; echo \"npm build\" > .husky/pre-push",
